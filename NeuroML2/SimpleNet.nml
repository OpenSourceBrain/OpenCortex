<neuroml xmlns="http://www.neuroml.org/schema/neuroml2"  xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.neuroml.org/schema/neuroml2 https://raw.github.com/NeuroML/NeuroML2/development/Schemas/NeuroML2/NeuroML_v2beta4.xsd" id="SimpleNet">
    <expTwoSynapse id="syn0" gbase="2nS" erev="0mV" tauDecay="10ms" tauRise="0.5ms"/>
    <izhikevich2007Cell id="RS" C="100 pF" v0="-60mV" k="3 nS_per_mV" vr="-60 mV" vt="-50 mV" vpeak="50 mV" a="0.01 per_ms" b="5 nS" c="-60 mV" d="400 pA"/>
    <poissonFiringSynapse id="poissonFiringSyn" averageRate="50 Hz" synapse="syn0" spikeTarget="./syn0"/>
    <network id="SimpleNet_network">
        <population id="Pop0" component="RS" size="5"/>
        <explicitInput target="Pop0[0]" input="poissonFiringSyn" destination="synapses"/>
        <explicitInput target="Pop0[1]" input="poissonFiringSyn" destination="synapses"/>
        <explicitInput target="Pop0[2]" input="poissonFiringSyn" destination="synapses"/>
        <explicitInput target="Pop0[3]" input="poissonFiringSyn" destination="synapses"/>
        <explicitInput target="Pop0[4]" input="poissonFiringSyn" destination="synapses"/>
    </network>
</neuroml>
